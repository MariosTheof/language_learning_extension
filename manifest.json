{

  "manifest_version": 2,
  "name": "Learnl",
  "version": "1.1",

  "description": "Displays pop up after interval with grammar questions.",

  "background": {
    "scripts": ["popup.js"]
  },
  "permissions" : [
    "storage",
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "Learnl",
    "default_popup": "settings.html"
  }
}
